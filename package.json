{
  "name": "semesterproject2",
  "version": "1.0.0",
  "description": "Velorum Auction Website",
  "main": "index.js",
  "scripts": {
    "clean": "rm -f css/styles.css",
    "build-css": "sass scss/main.scss css/styles.css --style=compressed",
    "watch-css": "sass --watch scss/main.scss css/styles.css",
    "watch": "live-server --port=8080 --open=src/home/index.html",
    "start": "npm-run-all --parallel watch-css watch",
    "build": "npm run clean && npm run build-css"
  },
  "keywords": [
    "auction",
    "website",
    "velorum",
    "bootstrap",
    "scss"
  ],
  "author": "Your Name",
  "license": "ISC",
  "devDependencies": {
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.82.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.3"
  }
}
